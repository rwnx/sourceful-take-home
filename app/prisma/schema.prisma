generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

enum ImageStatus {
  PENDING
  SUCCESS
  ERROR
}

model Image {
  id        String        @id @default(uuid())
  createdAt DateTime      @default(now())
  updatedAt DateTime      @updatedAt
  status    ImageStatus   @default(PENDING)
  groupId   String      
  prompt    String
  userId    String
  result    String?      
  error     String?      
}